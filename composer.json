{
  "name": "rdoddi/inline-raml",
  "description": "Generate RAML documents from annotated class files.",
  "keywords": ["raml", "library"],
  "homepage": "https://github.com/rdoddi/inline-raml",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ramesh Doddi",
      "email": "rdoddi@iremote.net"
    }
  ],
  "minimum-stability": "dev",
  "config": {
    "vendor-dir": "lib/vendor"
  },
  "require": {
    "pauldevelop/library-common": "*@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*",
    "phpdocumentor/phpdocumentor": "2.5.*@dev",
    "squizlabs/php_codesniffer": "1.*"
  },
  "autoload": {
    "psr-4": {
      "net\\iremote\\library\\inline\\raml\\": "src/class"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.0.x-dev"
    }
  }
}
